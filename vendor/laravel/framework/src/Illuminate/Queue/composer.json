{
    "name": "illuminate/queue",
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/console": "4.1.*",
        "illuminate/container": "4.1.*",
        "illuminate/http": "4.1.*",
        "illuminate/support": "4.1.*",
        "symfony/process": "2.4.*"
    },
    "require-dev": {
        "illuminate/events": "4.1.*",
        "aws/aws-sdk-php": "2.6.*",
        "iron-io/iron_mq": "1.5.*",
        "pda/pheanstalk": "2.1.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-0": {
            "Illuminate\\Queue": ""
        },
        "classmap": [
            "IlluminateQueueClosure.php"
        ]
    },
    "target-dir": "Illuminate/Queue",
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    },
    "suggest": {
        "illuminate/redis": "Allows use of the Redis queue driver."
    },
    "minimum-stability": "dev"
}
